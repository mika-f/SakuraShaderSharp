{
  "BaseDir": "./",
  "Includes": ["./**/*.cs"],
  "References": ["../../../Dependencies/plugins/SharpX.Library.ShaderLab.dll"],
  "Plugins": ["../../../Dependencies/plugins/SharpX.Compiler.ShaderLab.dll"],
  "Out": "../../../CompiledShaders/Avatars-Effect",
  "Target": "ShaderLab",
  "ShaderLab": {
    "ShaderVariants": [
      {
        "Name": "normal",
        "Directives": []
      },
      {
        "Name": "geom-voxel",
        "Directives": ["SHADER_GEOMETRY_VOXEL"]
      },
      {
        "Name": "geom-holograph",
        "Directives": ["SHADER_GEOMETRY_HOLOGRAPH"]
      }
    ]
  },
  "CodeCleanup": {
    "AllowList": {
      "Attributes": [
        "SharpX.Library.ShaderLab.Attributes.VertexShaderAttribute",
        "SharpX.Library.ShaderLab.Attributes.FragmentShaderAttribute",
        "SharpX.Library.ShaderLab.Attributes.GlobalMemberAttribute"
      ],
      "Methods": [],
      "Properties": [],
      "Fields": [],
      "Variables": []
    }
  }
}
